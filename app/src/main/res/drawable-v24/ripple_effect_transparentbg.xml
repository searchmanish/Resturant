<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:color="@color/colorAccent"
tools:targetApi="lollipop">

<item android:id="@android:id/mask">
    <shape android:shape="rectangle">
        <corners android:radius="@dimen/padding_extra_large_20"></corners>
        <solid android:color="@color/colorAccent" />
        <padding android:top="@dimen/padding_small_5"
            android:bottom="@dimen/padding_small_5"
            android:right="@dimen/padding_medium_10dp"
            android:left="@dimen/padding_medium_10dp"></padding>
    </shape>
</item>
</ripple>
